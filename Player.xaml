        <ImageBrush x:Key="PowerPoint" ImageSource="Resources/powerpoint.png"/>
        <ImageBrush x:Key="Publisher" ImageSource="Resources/publisher.png"/>
        <ImageBrush x:Key="Access" ImageSource="Resources/access.png"/>
        <ImageBrush x:Key="Outlook" ImageSource="Resources/outlook.png"/>
        <ImageBrush x:Key="OneNote" ImageSource="Resources/onenote.png"/>
    </Window.Resources>
</Window>
